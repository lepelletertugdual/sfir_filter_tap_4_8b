##     01.00. CLK
##     02.00. KEY
##     03.00. LED
## ######################################################################################################################################################################################################

## ######################################################################################################################################################################################################
## 01.00. CLK
## ######################################################################################################################################################################################################
set_property -dict { PACKAGE_PIN Y9 IOSTANDARD LVCMOS33 } [get_ports { OSC_100M }];

## ######################################################################################################################################################################################################
## 02.00. KEY
## ######################################################################################################################################################################################################
set_property -dict { PACKAGE_PIN P16 IOSTANDARD LVCMOS33 } [get_ports { BTNC }];

## ######################################################################################################################################################################################################
## 03.00. LED
## ######################################################################################################################################################################################################
set_property -dict { PACKAGE_PIN U14 IOSTANDARD LVCMOS33 } [get_ports { ALIVE }];
set_property -dict { PACKAGE_PIN U19 IOSTANDARD LVCMOS33 } [get_ports { MMCM_LOCKED }];
set_property -dict { PACKAGE_PIN T22 IOSTANDARD LVCMOS33 } [get_ports { FAILURE }];

## ######################################################################################################################################################################################################
## EOF
## ######################################################################################################################################################################################################